defmodule Santagoras do
  alias Util.Counter

  def parse_input(input) do
    lines =
      input
      |> String.split("\n", trim: true)

    lines
    |> Enum.group_by(
      fn
        "(" <> _ -> :locations
        _ -> :route
      end,
      fn
        "(" <> _ = s ->
          Regex.scan(~r/\d+/, s)
          |> Enum.map(fn [n] -> String.to_integer(n) end)
          |> List.to_tuple()

        s ->
          s |> String.to_integer()
      end
    )
  end

  def covered_area({ax, ay}, {bx, by}) do
    xs =
      if ax < bx do
        ax..(bx - 1)
      else
        ax..(bx + 1)
      end

    ys =
      if ay < by do
        ay..(by - 1)
      else
        ay..(by + 1)
      end

    for(y <- ys, do: for(x <- xs, do: {x, y}))
    |> List.flatten()
  end

  def pretty_print(%Counter{items: count}) do
    {max_x, _} = count |> Map.keys() |> Enum.max_by(fn {x, _} -> x end)
    {_, max_y} = count |> Map.keys() |> Enum.max_by(fn {_, y} -> y end)

    max_y..0
    |> Enum.each(fn y ->
      ("#{y}\t|" <>
         (0..max_x
          |> Stream.map(fn x ->
            Map.get(count, {x, y}, 0)
            |> case do
              0 -> "."
              i when is_integer(i) -> Integer.to_string(i)
            end
          end)
          |> Enum.join("")))
      |> IO.puts()
    end)

    IO.puts("")
  end

  def solve(input, acc \\ %Counter{})

  def solve(%{route: route}, acc) when length(route) < 2, do: acc

  def solve(%{locations: locs, route: [a, b | t]}, acc) do
    {ax, ay} = Enum.at(locs, a)
    {bx, by} = Enum.at(locs, b)

    new = covered_area({ax, ay}, {bx, by}) |> Counter.new()

    solve(%{locations: locs, route: [b | t]}, Counter.merge(acc, new))
  end
end
