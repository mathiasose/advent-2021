defmodule SmartSnailor do
  def travel({0, 0}, :north, n) do
    nn = div(n, 45)
    rr = rem(n, 45)

    if nn == 0 do
      NaiveSnailor.travel({0, 0}, :north, n)
    else
      NaiveSnailor.travel({nn * 30 - 3, 3 + nn * 15}, :north, rr)
    end
  end
end
