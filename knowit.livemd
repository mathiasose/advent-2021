defmodule Layer do
  import Bitwise

  defstruct [:data]

  @width 20
  @empty_layer for _ <- 0..@width, do: 0

  def new({x, w}) do
    %Layer{
      data:
        for(
          i <- 0..@width,
          do:
            if i >= x and i < x + w do
              1
            else
              0
            end
        )
    }
  end

  def empty, do: %Layer{data: @empty_layer}

  def merge(%Layer{} = a, %Layer{} = b) do
    if overlaps?(a, b) do
      throw(:error)
    else
      %Layer{
        data:
          Stream.zip(a.data, b.data)
          |> Enum.map(fn {aa, bb} -> aa ||| bb end)
      }
    end
  end

  def overlaps?(%Layer{data: a}, %Layer{data: b}) do
    Stream.zip(a, b)
    |> Stream.map(fn {aa, bb} -> aa &&& bb end)
    |> Enum.any?(fn x -> x == 1 end)
  end

  def is_supported?(%Layer{} = top, %Layer{} = bottom) do
    w = Enum.sum(top.data)
    x = Enum.find_index(top.data, fn x -> x == 1 end)
    odd? = rem(w, 2) == 1
    h = div(w, 2)

    left_half =
      if odd? do
        Layer.new({x, h + 1})
      else
        Layer.new({x, h})
      end

    right_half =
      if odd? do
        Layer.new({x + h, h + 1})
      else
        Layer.new({x + h, h})
      end

    overlaps?(left_half, bottom) and overlaps?(right_half, bottom)
  end

  def stacks?(%Layer{} = top, %Layer{} = bottom) do
    cond do
      not overlaps?(top, bottom) -> false
      is_supported?(top, bottom) -> true
      true -> false
    end
  end
end

defmodule PackStack do
  defstruct layers: [Layer.empty()]

  def add_next(next, stack, lost \\ 0)

  def add_next(%Layer{} = next, %PackStack{layers: []}, 0), do: {%PackStack{layers: [next]}, 0}

  def add_next(%Layer{} = next, %PackStack{} = stack, lost) do
    stack.layers
    |> Enum.find_index(fn layer -> Layer.overlaps?(next, layer) end)
    |> case do
      nil ->
        combined = Enum.at(stack.layers, -1) |> Layer.merge(next)
        updated_layers = stack.layers |> List.replace_at(-1, combined)

        {%PackStack{layers: updated_layers}, lost}

      i when i >= 0 ->
        bottom = Enum.at(stack.layers, i)

        if Layer.is_supported?(next, bottom) do
          target_layer = i - 1

          updated_layers =
            if target_layer == -1 do
              [next] ++ stack.layers
            else
              combined = Enum.at(stack.layers, target_layer) |> Layer.merge(next)
              stack.layers |> List.replace_at(target_layer, combined)
            end

          {%PackStack{layers: updated_layers}, lost}
        else
          {stack, lost + 1}
        end
    end
  end

  def parse_input(input) do
    input
    |> String.trim()
    |> String.split("\n")
    |> Enum.map(fn s ->
      [x, w] = s |> String.split(",")
      {x, ""} = Integer.parse(x)
      {w, ""} = Integer.parse(w)
      {x, w}
    end)
    |> Enum.map(fn {x, w} -> Layer.new({x, w}) end)
  end

  def pretty_print(%PackStack{layers: layers}, opts \\ []) do
    bottom = Keyword.get(opts, :bottom, -1)

    layers
    |> Enum.slice(-bottom..-1)
    |> Enum.map(fn layer ->
      layer.data
      |> Enum.join("")
      |> String.replace("0", " ")
      |> String.replace("1", "█")
      |> IO.puts()
    end)
  end
end
