defmodule Util.Counter do
  defstruct items: %{}

  def new(items, default_keys \\ []) do
    defaults = default_keys |> Enum.map(fn key -> {key, 0} end) |> Enum.into(%{})

    %Util.Counter{
      items:
        items
        |> Enum.reduce(defaults, fn x, acc -> acc |> Map.update(x, 1, fn v -> v + 1 end) end)
    }
  end

  def merge(%Util.Counter{items: a}, %Util.Counter{items: b}) do
    %Util.Counter{items: Map.merge(a, b, fn _k, v1, v2 -> v1 + v2 end)}
  end

  def most_common(counter, n \\ 10) do
    counter.items
    |> Map.to_list()
    |> Enum.sort(fn {_k1, val1}, {_k2, val2} -> val1 > val2 end)
    |> Enum.take(n)
  end
end

Util.Counter.new(~w/A B C B C D/, ["F"]) |> Util.Counter.merge(Util.Counter.new(~w/A B C/))
